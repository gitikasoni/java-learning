package com.examples.java;

public class Employee {

	/*
	 * private String name; private String email; private int age; private
	 * double salary; private int mobile;
	 */

	String name;
	String email;
	int age;
	double salary;
	long mobile;

	public Employee() {

		System.out.println("Inside constructor ");
	}

	public Employee(String name, String email, int age, double salary,
			long mobile) {
		this.name = name;
		this.email = email;
		this.age = age;
		this.salary = salary;
		this.mobile = mobile;
		System.out.println(" Inside parameterized constructor ... 5 values ");
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	public double getSalary() {
		return salary;
	}

	public void setSalary(double salary) {
		this.salary = salary;
	}

	public long getMobile() {
		return mobile;
	}

	public void setMobile(long mobile) {
		this.mobile = mobile;
	}

	@Override
	public String toString() {
		return "Employee [name=" + name + ", email=" + email + ", age=" + age
				+ ", salary=" + salary + ", mobile=" + mobile + "]";
	}

	public static void main(String[] args) {

		Employee e1 = new Employee();
		System.out.println("Printing default values e1 : " + e1);

		e1.setAge(12);
		e1.setEmail("abc@gmail.com");
		e1.setMobile(6898202);
		e1.setName("Ram");
		e1.setSalary(100);
		System.out.println("Print the set values e1: " + e1);

		Employee e2 = new Employee("Ramesh", "gkjd@kdmf.com", 23, 1000, 68990);
		System.out.println("Printing e2 : " + e2);

	}
}
