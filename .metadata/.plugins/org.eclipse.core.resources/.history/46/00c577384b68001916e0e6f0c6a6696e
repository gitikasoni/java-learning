package com.examples.java;

public class ArrayListExample {

	public static void main(String args[]) {
		int num[] = new int[5];
		System.out.println(num);
		System.out.println("Array num length :" + num.length);
		for (int i = 0; i < num.length; i++) {
			System.out.println(num[i]);
		}

		System.out.println("Assigning value to array");
		num[0] = 10;
		num[1] = 11;
		num[2] = 12;
		num[3] = 13;
		num[4] = 14;
		num[5] = 100;
		num[6] = 56;
		for (int i = 0; i < num.length; i++) {
			System.out.println(num[i]);
		}

	}

}
