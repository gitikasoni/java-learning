package com.examples.collection;

import java.util.HashSet;
import java.util.LinkedHashSet;
import java.util.Set;
import java.util.TreeSet;

public class SetGenericsExample {
	
	//unique + random order
	HashSet<String> hash = new HashSet<String>();
	
	
	//unique+ asc/desc
	TreeSet<String> tree = new TreeSet<String>();
	
	hash.add("This");
	hash.add("is");
	hash.add("HashSet");
	hash.add("example");
	
	System.out.print("HashSet Example -- Printing values " +hash);

	for(String str : hash)
	{
		System.out.println("HashSet example using for each" +str);
	}
	

	//unique + insertion order
		LinkedHashSet<String> set2 = new LinkedHashSet<String>();
		set2.add("new");
		set2.add("policy");
		set2.add("test");
		
		System.out.print("LinkedHashSet example-- " +set2);

		for(String str1: set2)
		{
			System.out.println("HashSet example using for each" + str1);
		

}
