package com.examples.java;

import java.util.HashMap;
import java.util.Map;

public class MapExample {

	public static void main(String[] args) {

		
		Map map = new HashMap();
		
		map.put("IN", "India");
		map.put("UK", "Unitred Kingdom");
		map.put("USA", "United States of America");
		map.put("FR", "France 1");
		map.put("FR", "France 2");
		map.put("FR", "Adarak");
		map.put("UAE", "United Arab Emir");
		map.put("Khichdi", "Dal");
		
		System.out.println("Printng Map : "+map);
		System.out.println("Printng Map size: "+map.size());
	
	
		
	}

}
